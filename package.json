{
  "name": "eslint-config",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "lint": "eslint .",
    "publish:js": "npm publish --workspace=@cabe/eslint-config --access=public",
    "publish:ts": "npm publish --workspace=@cabe/eslint-config-typescript --access=public",
    "publish:all": "npm publish --workspaces --access=public"
  },
  "devDependencies": {
    "eslint": "^9.28.0",
    "typescript": "^5.9.3"
  }
}
